  <body>
    <script>
      var app = ''
      var appios = ''
      var url = location.href
      app = url.replace('https://tyhy.hzxc.gov.cn:8443/1do/shortMessage'+
      '/MyHtml.html?SHOW_ID=', '1call://open/1do/')
      appios = url.replace('https://tyhy.hzxc.gov.cn:8443/1do/shortMessage'+
      '/MyHtml.html?SHOW_ID=', '1call://openOneDoDetail?showId=')
	var u = navigator.userAgent, app = navigator.appVersion; 
    var isAndroid = u.indexOf('Android') > -1 || u.indexOf('Linux') > -1;//g 
    var isIOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端 
    if (isAndroid) {
       // window.location.href = 'appDownload.html'; Android
          location.href = app
        } 
    else if (isIOS) { 
          location.href = appios } //IOS		
     else{
          location.href = appios
     }     
    </script>
   
